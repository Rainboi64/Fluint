<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        Width="1000" Height="600"
        x:Class="Fluint.SDK.Commands.Splitter.GUI.MainWindow"
        Title="Fluint.SDK.Commands.Splitter">
  <DockPanel>
    
    <Menu DockPanel.Dock="Top">
      <MenuItem Header="_File">
        <MenuItem Header="_Open..." Name="OpenMenuItem"/>
        <Separator/>
        <MenuItem Header="_Exit"/>
      </MenuItem>
    </Menu>
    <DockPanel DockPanel.Dock="Left">
      <ListBox Name="Namespaces" DockPanel.Dock="Top" Width="250"/>
      <ListBox Name="NamespaceMembers" DockPanel.Dock="Bottom" Width="250"/>
    </DockPanel>
    <DockPanel DockPanel.Dock="Right">
      <TextBlock Name="TypeTitle" FontSize="24" Margin="10" FontWeight="Bold"/>
    </DockPanel>
  </DockPanel>
</Window>
