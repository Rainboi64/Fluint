{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "."
        }
      ],
      "dest": "docs/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/api/**.yml",
          "docs/api/index.md"
        ]
      },
      {
        "files": [
          "docs/articles/**.md",
          "docs/articles/**/toc.yml",
          "docs/toc.yml",
          "docs/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "docs/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "docs/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/_site/**"
        ]
      }
    ],
    "dest": "docs/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template":["docs/templates/mod_default","docs/templates/darkfx"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
